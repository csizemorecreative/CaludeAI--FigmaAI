{
  "name": "@oclc/design-system-components",
  "version": "1.2.0",
  "description": "OCLC's design system React component library",
  "license": "See license.md",
  "source": "lib/lib.package.js",
  "main": "./dist/cjs/lib/lib.package.js",
  "module": "./dist/lib/lib.package.js",
  "types": "./dist/lib/lib.package.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "/dist",
    "design-tokens/*.json",
    "design-tokens/new-palette/*.json"
  ],
  "repository": {
    "type": "git",
    "url": "http://git.ent.oclc.org/scm/dsys/component-library.git"
  },
  "scripts": {
    "dev": "microbundle watch --jsx React.createElement lib/*.js",
    "build:lib": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "deploy-storybook": "storybook-to-aws-s3 --bucket-path=oclc-ds-storybook.org/",
    "deploy-storybook-pg": "storybook-to-aws-s3 --bucket-path=oclc-design-system-playground.org/",
    "deploy-storybook-prod": "storybook-to-aws-s3 --bucket-path=oclc-design-system-prod.org/",
    "deploy-storybook-v2": "storybook-to-aws-s3 --bucket-path=oclc-design-system-version-poc.org/v2",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@storybook/addon-a11y": "^7.1.0",
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-mdx-gfm": "^7.1.0",
    "@storybook/addon-themes": "^8.1.2",
    "@storybook/addons": "^7.1.0",
    "@storybook/icons": "^1.2.9",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "axe-playwright": "^1.2.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^9.0.11",
    "jest": "^28.1.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-localstorage-mock": "^2.4.21",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.2.2",
    "microbundle": "^0.15.0",
    "prettier": "2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.0",
    "typescript": "^5.4.2",
    "@mui/x-data-grid-generator": "^6.19.11",
    "date-fns": "^2.29.3",
    "react-svg": "^16.1.19"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.18",
    "@mui/x-data-grid-pro": "^6.19.11",
    "@mui/x-date-pickers-pro": "^6.19.11",
    "mdi-material-ui": "^7.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://artifact-m1.shr.oclc.org/artifactory/api/npm/npm/"
  }
}
